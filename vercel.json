{
  "buildCommand": "npm run build",
  "outputDirectory": "dist/public", 
  "framework": null,
  "installCommand": "npm install",
  "functions": {
    "server/index.ts": {
      "runtime": "@vercel/node@5.3.0"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/server/index.ts"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}